- model_class = @photo.class
= render 'photos/breadcrumbs', :locals=>{:action=>:show}


%dl.dl-horizontal
  %dd= image_tag(@photo.image.url)
  %dd= @photo.blurb



-#/
  %p#notice= notice

  %p
    %b Uid:
    = @photo.uid
  %p
    %b Permalink:
    = @photo.permalink
  %p
    %b Name:
    = @photo.name
  %p
    %b Keywords:
    = @photo.keywords
  %p
    %b Blurb:
    = @photo.blurb
  %p
    %b State:
    = @photo.state
  %p
    %b Type:
    = @photo.type
  %p
    %b Image url:
    = @photo.image_url
  %p
    %b Image uid:
    = @photo.image_uid
  %p
    %b Image name:
    = @photo.image_name
  %p
    %b Topic:
    = @photo.topic

  = link_to 'Edit', edit_photo_path(@photo)
  \|
  = link_to 'Back', photos_path    
