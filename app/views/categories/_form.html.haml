= simple_form_for(@category) do |f|
  = f.error_notification

  .form-inputs
    = f.input :uid
    = f.input :permalink
    = f.input :name
    = f.input :keywords
    = f.input :blurb, :input_html=>{:class=>"wysihtml5"} 
    = f.input :body, :input_html=>{:class=>"wysihtml5"} 
    = f.input :state
    = f.input :type
    = f.association :photo

  .form-actions
    = f.button :submit
